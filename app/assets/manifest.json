{
  "manifest_version": 2,
  "name": "Tabs Manager",
  "version": "0.1.0",
  "description": "This extension allows to list and manipulate currently open tabs",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "browser_action": {
    "default_icon": "icons/icon-96.png",
    "default_title": "Tabs Manager",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs"
  ]
}
