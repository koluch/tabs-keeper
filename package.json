{
  "name": "tabs-keeper",
  "description": "This extension allows to list, manipulate and save currently open tabs",
  "private": true,
  "author": "Nikolai Mavrenkov <koluch@koluch.ru> (https://koluch.ru)",
  "version": "0.2.6",
  "repository": "https://github.com/koluch/tabs-keeper",
  "scripts": {
    "start": "brunch watch --server",
    "start:production": "NODE_ENV=testing brunch watch --server --production",
    "clean": "rm -rf ./dist",
    "pack": "VERSION=`node -p -e \"require('./package.json').version\"` && cd dist && zip -r ../tabs-keeper-${VERSION}.zip ./*",
    "build": "npm run clean && tsc && brunch build --production && npm run pack",
    "prettier": "prettier --write 'app/**/*.ts?(x)'",
    "test:lint": "eslint ./app/**/*.ts",
    "test": "npm run test:lint"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "preact": "^8.2.9"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/node": "^10.5.2",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "babel-brunch": "^6.1.1",
    "babel-preset-env": "^1.7.0",
    "brunch": "^2.10.17",
    "clean-css-brunch": "^2",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "less-brunch": "^2.10.0",
    "prettier": "^1.15.3",
    "typescript": "^3.2.4",
    "uglify-js-brunch": "^2",
    "web-ext-types": "^2.1.0"
  }
}
