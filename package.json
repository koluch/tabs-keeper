{
  "name": "brunch-app",
  "description": "Brunch.io application",
  "private": true,
  "author": "Brunch",
  "version": "0.1.0",
  "repository": "",
  "scripts": {
    "start:debug": "brunch watch --server",
    "start": "brunch watch --server --production",
    "clean": "rm -rf ./dist",
    "pack": "VERSION=`node -p -e \"require('./package.json').version\"` && zip -r tabs-keeper-${VERSION}.zip dist",
    "build": "npm run clean && tsc && brunch build --production && npm run pack"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "preact": "^8.2.9"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/node": "^10.5.2",
    "babel-brunch": "^6.1.1",
    "babel-preset-env": "^1.7.0",
    "brunch": "^2",
    "clean-css-brunch": "^2",
    "less-brunch": "^2.10.0",
    "typescript": "^2.9.2",
    "uglify-js-brunch": "^2",
    "web-ext-types": "^2.1.0"
  }
}
